function init(self)
	timer.delay(1, true, friend_spawn)
	timer.delay(1, true, enemy_spawn)
end

function enemy_spawn(self)
	factory.create("#spawner", go.get_position("/enemy"), nil, {id = 2})
end

function friend_spawn(self)
	factory.create("#spawner", go.get_position("/friend"), nil, {id = 1})
end